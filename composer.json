        "drupal/file_download_link": "^2",
        "drupal/formtips": "^1.5",
        "drupal/fullcalendar_view": "5.1.14",
        "drupal/google_analytics": "^4.0",
        "drupal/google_tag": "^2.0",
        "drupal/honeypot": "^2.1",
        "drupal/hook_event_dispatcher": "^3.3",
        "drupal/image_url_formatter": "^1.0",
        "drupal/image_widget_crop": "^2.3",
        "drupal/inline_entity_form": "^1.0@RC",
        "drupal/jquery_ui_accordion": "^2.0",
        "drupal/layout_builder_ids": "^2.0@RC",
        "drupal/layout_builder_modal": "^1.2",
        "drupal/layout_builder_styles": "^2.0",
        "drupal/lazy_views": "1.0.0",
        "drupal/linkchecker": "^2.0",
        "drupal/linkit": "^6.0@beta",
        "drupal/linkit_media_library": "^1.0",
        "drupal/login_destination": "^2.0@beta",
        "drupal/ludwig": "^2.0",
        "drupal/mail_login": "^2.4",
        "drupal/mailchimp": "^2.0",
        "drupal/mailsystem": "^4.4",
        "drupal/media_directories": "^2.0",
        "drupal/media_entity_file_replace": "^1.0",
        "drupal/media_entity_link": "^2.0",
        "drupal/menu_item_role_access": "^2.0",
        "drupal/menu_link_attributes": "^1.2",
        "drupal/metatag": "^1.16",
        "drupal/multiple_fields_remove_button": "^2.2",
        "drupal/nbsp": "^2.3",
        "drupal/node_revision_delete": "^2.0@RC",
        "drupal/paragraph_view_mode": "^3.1",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/permissions_filter": "^1.2",
        "drupal/protected_pages": "^1.4",
        "drupal/publication_date": "^2.0@beta",
        "drupal/quick_node_block": "^1.3",
        "drupal/quick_node_clone": "^1.14",
        "drupal/rabbit_hole": "^1.0@beta",
        "drupal/recaptcha": "^3.0",
        "drupal/recaptcha_v3": "^1.4",
        "drupal/redirect": "^1.6",
