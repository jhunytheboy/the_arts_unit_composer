        "drupal/ckeditor": "^1.0",
        "drupal/ckeditor_accordion": "^2.0",
        "drupal/ckeditor_bootstrap_buttons": "*",
        "drupal/ckeditor_bootstrap_table": "^1.2",
        "drupal/ckeditor_bs_grid": "^2.0",
        "drupal/ckeditor_div_manager": "^2.1",
        "drupal/ckeditor_iframe": "^2.1",
        "drupal/color_field": "^3.0",
        "drupal/conditional_fields": "^4.0@alpha",
        "drupal/config_backup": "^1.0",
        "drupal/contact_block": "^1.5",
        "drupal/content_moderation_notifications": "^3.3",
        "drupal/core-composer-scaffold": "^9.1.5",
        "drupal/core-project-message": "^9.1.5",
        "drupal/core-recommended": "^9.1.5",
        "drupal/csv_importer": "^1.11",
        "drupal/csv_serialization": "^3.0",
        "drupal/ctools": "3.14",
        "drupal/devel": "^5.0",
        "drupal/diff": "1.3",
        "drupal/ds": "^3.9",
        "drupal/entity_browser": "^2.6",
        "drupal/entity_embed": "^1.1",
        "drupal/entity_export_csv": "^2.0@beta",
        "drupal/entity_hierarchy": "^3.0",
