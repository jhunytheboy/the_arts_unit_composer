        "drupal/rip": "^1.1",
        "drupal/search_api": "^1.18",
        "drupal/search_api_exclude_entity": "^1.3",
        "drupal/search_api_page": "^1.0@beta",
        "drupal/search_api_sorts": "1.x-dev@dev",
        "drupal/select2": "^1.15",
        "drupal/selective_better_exposed_filters": "^3.0",
        "drupal/simple_sitemap": "^4.1",
        "drupal/smart_date": "^4.0",
        "drupal/svg_image": "^3.0",
        "drupal/swiftmailer": "^2.2",
        "drupal/symfony_mailer": "^1.2",
        "drupal/taxonomy_manager": "^2.0",
        "drupal/term_csv_export_import": "^4.0",
        "drupal/term_merge": "^2.0@beta",
        "drupal/token": "^1.15",
        "drupal/token_filter": "^1.4",
        "drupal/tvi": "^2.0@alpha",
        "drupal/twig_field_value": "^2.0",
        "drupal/twig_tweak": "^2.8",
        "drupal/typed_data": "^1.0@beta",
        "drupal/unpublished_404": "^2.0",
        "drupal/upgrade_status": "^4.3",
        "drupal/vapn": "^1.5",
        "drupal/view_mode_page": "^4.0",
        "drupal/view_unpublished": "^1.0",
        "drupal/views_bulk_edit": "^2.5",
        "drupal/views_bulk_operations": "^4.1",
        "drupal/views_conditional": "*",
        "drupal/views_data_export": "^1.0",
        "drupal/views_expandable_table": "^1.0@RC",
        "drupal/views_infinite_scroll": "^2.0",
        "drupal/viewsreference": "^1.8",
        "drupal/webform": "^6.2",
        "drupal/webform_entity_view": "^1.0",
        "drupal/webform_spam_words": "^2.0",
        "drupal/webform_xlsx_export": "^1.0",
        "drupal/weight": "^3.3",
        "drupal/yaml_editor": "^1.1",
        "drush/drush": "11.*",
        "kint-php/kint": "^3.3",
        "vimeo/vimeo-api": "*",
        "wikimedia/composer-merge-plugin": "^2.1"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "cweagans/composer-patches": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library",
                "type:bower-asset",
                "type:npm-asset"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        },
        "installer-types" : ["bower-asset", "npm-asset"],
        "patches" : {
            "drupal/ds": {
                "Undefined index: #type in _ds_field_ui_table_layout()": "https://www.drupal.org/files/issues/2021-07-29/ds-undefined-index-type-3091319-14.patch"
            },
            "drupal/ckeditor_accordion": {
                "HTML in tab head (https://www.drupal.org/project/ckeditor_accordion/issues/3355048)": "https://git.drupalcode.org/project/ckeditor_accordion/-/merge_requests/7.diff"
            },
            "drupal/backup_migrate" : {
                "Add Drush command for running a quick backup (https://www.drupal.org/project/backup_migrate/issues/3223057)": "https://www.drupal.org/files/issues/2021-12-02/3223057-17.patch"
            },
            "drupal/linkchecker" : {
                "Remove redirect checker" : "private/patches/linkchecker-remove-redirect-message.patch"
            },
            "drupal/fullcalendar_view" : {
                "Disable ajaxcomplete (based on https://www.drupal.org/project/fullcalendar_view/issues/3323399 )": "private/patches/fullcalendar_view-disable-ajaxcomplete.patch"
            },
            "drupal/core" : {
                "New option for Views page displays to use the admin theme (https://www.drupal.org/project/drupal/issues/2719797)" : "https://www.drupal.org/files/issues/2023-05-05/2719797-201.patch"
            }
        },
        "merge-plugin": {
            "include": [
                "web/modules/contrib/webform/composer.libraries.json",
                "web/modules/contrib/media_directories/composer.libraries.json"
            ]
        }
    }
}
